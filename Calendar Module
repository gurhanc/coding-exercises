import calendar
input_month,input_day,input_year = list(map(int,input().strip().split()))
x = calendar.weekday(input_year, input_month, input_day)
if x == 0:
    print("MONDAY")
elif x == 1:
    print("TUESDAY")
elif x == 2:
    print("WEDNESDAY")
elif x == 3:
    print("THURSDAY")
elif x == 4:
    print("FRIDAY")
elif x == 5:
    print("SATURDAY")
else:
    print("SUNDAY")
