function palindrome(str) {
  // Good luck!
  let arr = [];
  str = str.toLowerCase().replace(/[^\w]|_/g,"");
  console.log(str)
  for (let i=str.length-1;i>=0;i--) {
    arr.push(str[i]);
  }
  let strPal = arr.join("");
  console.log(strPal)
  if (strPal == str) {
    return true;
  }
  else { return false;} 
}
