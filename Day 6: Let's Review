n = int(input())
for i in range(n):
    myevenList = []
    myoddList = []
    myString = input().strip()
    stringLength = len(myString)
    for i in range(0,stringLength,2):
        myevenList.append(myString[i])
    print("".join(myevenList), end=" ")
    for i in range(1,stringLength,2):
        myoddList.append(myString[i])
    print("".join(myoddList))    
