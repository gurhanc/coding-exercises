function getLetter(s) {
    let letter;
    // Write your code here
    var set1 = ["a","e","i","o","u"];
    var set2 = ["b","c","d","f","g"];
    var set3 = ["h","j","k","l","m"];
    if (set1.includes(s[0])) {
        letter = "A";
    }
    else if (set2.includes(s[0])) {
        letter = "B";
    }
    else if (set3.includes(s[0])) {
        letter = "C";
    }
    else {
        letter = "D";
    }
    return letter;
}
