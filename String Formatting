def print_formatted(number):
    # your code goes here
    for i in range(1,n+1):
        width = len(bin(n))-2
        a = str(i).rjust(width)
        b = str(oct(i).replace("0o","")).rjust(width)
        c = str(hex(i).upper().replace("0X","")).rjust(width)
        d = str(bin(i).replace("0b","")).rjust(width)
        print(a,b,c,d)
